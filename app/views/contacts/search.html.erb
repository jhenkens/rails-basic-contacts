<h1>Search results</h1>
<h2>Query: <%= @query %></h2>
<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.first_name %></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.company %></td>
        <td><%= link_to 'Show', contact %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @contacts %>

